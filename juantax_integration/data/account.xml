    <record id="sequence_sale_invoice_transaction" model="ir.sequence">
        <field name="name">Sale invoices transaction sequence</field>
        <field name="code">sale.invoice.transaction.action</field>
        <field name="prefix">SALE-Transaction/%(range_year)s/</field>
        <field eval="1" name="number_next"/>
        <field eval="1" name="number_increment"/>
        <field eval="True" name="use_date_range"/>
        <field eval="False" name="company_id"/>
        <field name="padding">4</field>
    </record>

